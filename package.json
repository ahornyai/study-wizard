    "@types/express-session": "^1.17.4",
    "@types/mysql": "^2.15.19",
    "@types/sequelize": "^4.28.10",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-session-sequelize": "^2.3.0",
    "mysql2": "^2.3.3",
